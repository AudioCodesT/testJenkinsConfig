    <hudson.tasks.Shell>
      <command>artifactory_host=${ARTIFACTORY_HOST}
output_folder=${DEPENDENCIES_FOLDER}
User=&quot;$artifactory_user:$artifactory_password&quot;

NEWTAG=`echo $GERRIT_REFNAME | awk -F/ &apos;{print $NF}&apos;`
Major=`echo ${NEWTAG} | cut -d_ -f1`
Build=`echo ${NEWTAG} | cut -d_ -f2,3`

echo NEWTAG=${NEWTAG} &gt; generated_version.properties
echo Major=$Major &gt;&gt; generated_version.properties
echo Build=$Build &gt;&gt; generated_version.properties

BL=`git describe --tags --first-parent`
echo BL ver is: ${BL}
echo COMP_VER=${BL} &gt;&gt; generated_version.properties

echo &quot;#define EXTERNAL_VERSION \&quot;$BL\&quot;&quot; &gt; $WORKSPACE/rg/pkg/include/external_version_data.h

cd rg
sudo chmod -R 755 .
sudo chown -R centos:centos .

./dceBuild.sh  --board=$BOARD --fast=no --full=yes
</command>
    </hudson.tasks.Shell>
