  </triggers>
  <concurrentBuild>false</concurrentBuild>
  <builders>
    <hudson.tasks.Shell>
      <command># we build only if jenkins checked out on the last commit in branch, else exit 0
LastGerritCommitHash=$( git ls-remote  | grep refs/heads/$GERRIT_BRANCH  | cut -f 1 | head -n 1 )
echo $LastGerritCommitHash
echo $GERRIT_PATCHSET_REVISION
if [ &quot;$LastGerritCommitHash&quot; == &quot;$GERRIT_PATCHSET_REVISION&quot; ]
then
	LastTag=$(git describe --tags --abbrev=0 --first-parent)
	NEWTAG=${LastTag}
	commits=($(git log --reverse ${LastTag}..HEAD --pretty=format:&quot;%H&quot;))
 	#get first commit hash + user name of that commit
  	array_index=0
  	while [[ &quot;${array_index}&quot; != &quot;${#commits[@]}&quot; ]]
  	do
		commit_id=${commits[$array_index]}
     	Subject=$(git log --pretty=format:&quot;%s&quot; -n 1 ${commit_id})	
     	User=$(git log --pretty=format:&quot;%cn&quot; -n 1 ${commit_id})
        #get next commit user name
        ((array_index+=1))
        next_commit_user=$(git log --pretty=format:&quot;%cn&quot; -n 1 ${commits[$array_index]})
     	if [[ &quot;${User}&quot; != &quot;${next_commit_user}&quot; ]]
     	then
        	NEWTAG=`echo $NEWTAG | sed -r &apos;s/(.*)\.[^\d]+$/\1/&apos;`.`echo $NEWTAG | awk -F. &apos;{print $NF+1}&apos;`
     		git tag -a -m &quot;Jenkins auto tag after ${User} commit&quot; ${NEWTAG} ${commit_id}
     	fi
  		
  	done

else
	echo &quot;******   This is not the last commit. Exit without building  ******&quot;
fi</command>
    </hudson.tasks.Shell>
  </builders>
  <publishers>
