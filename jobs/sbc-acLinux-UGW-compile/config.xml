  <builders>
    <hudson.tasks.Shell>
      <command>BL=`git describe --tags --first-parent | sed s/-/_/g`
echo BL ver is: ${BL}
echo COMP_VER=${BL} &gt; generated_version.properties

echo "#define EXTERNAL_VERSION \"$BL\"" &gt; $WORKSPACE/acLinuxUGW_external_version_data.h
</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.3.0">
      <info>
        <propertiesFilePath>generated_version.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Shell>
      <command>
sudo chmod -R 755 .
sudo chown -R centos:centos .

sudo mount -a

./buildKernel.sh --board=MP5XXNG || exit 1

cp  $WORKSPACE/$EXTERNAL_VERSION/external_version_data.h $WORKSPACE/acLinuxUGW_external_version_data.h

if (( `du MP5XXNG.vmlinux.gz | cut -f1` &lt; 18000 )); then
  echo "MP5XXNG.vmlinux.gz Image is too small.. exit with error"
  exit -1
fi
</command>
    </hudson.tasks.Shell>
