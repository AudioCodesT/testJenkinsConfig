  <builders>
    <hudson.tasks.Shell>
      <command>BL=`git describe --tags`
echo BL ver is: ${BL}
echo COMP_VER=${BL} &gt; generated_version.properties

echo &quot;#define EXTERNAL_VERSION \&quot;$BL\&quot;&quot; &gt; $WORKSPACE/external_version_data.h
</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.1.6">
      <info>
        <propertiesFilePath>generated_version.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Shell>
      <command>CurrentPath=`pwd`
echo &quot;machine test-gerrit login jenkins password $jpass&quot; &gt;~/.netrc
cd ../..
TPBranch=&quot;7.2.150&quot;
if [ ! -d .git ]; then
  git -c http.sslverify=false clone -n https://test-gerrit/TP/GWApp GWApp
  mv GWApp/.git .
  rm -rf GWApp
fi
git checkout -f origin/$GERRIT_BRANCH -- \
  TrunkPackRam/Bsps/Common/Include/HAWatchdog.h \
  TrunkPackRam/Bsps/BspOcteon/Include/HWwatchdog.h \
  TrunkPackRam/Bsps/BspOcteon/Include/SPI_Driver.h \
  TrunkPackRam/PSTN/ph/PstnInt.h \
  OpenSource/pkgs Modules/Apps
cd $CurrentPath

sudo chmod -R 755 .
sudo chown -R centos:centos .

./build_acLinux --board=${BOARD} || exit 1

cp  $WORKSPACE/external_version_data.h $WORKSPACE/acLinux_external_version_data.h

if (( `du ${BOARD}.vmlinux.64.gz | cut -f1` &lt; 8000 )); then
  echo &quot;${BOARD}.vmlinux.64.gz Image too small.. exit with error&quot;
  exit -1
fi
</command>
    </hudson.tasks.Shell>
