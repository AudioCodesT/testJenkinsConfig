    <hudson.tasks.Shell>
      <command>#GERRIT_BRANCH=`git branch --no-color -r --contains HEAD | grep -v -e &apos;-&gt;&apos; | head -1 | awk -F&apos;/&apos; &apos;{print $NF}&apos;`

CurrentPath=`pwd`
echo &quot;machine test-gerrit login jenkins password $jpass&quot; &gt;~/.netrc
cd ../..
if [ ! -d .git ]; then
  git -c http.sslverify=false clone --depth 10 -n -b $GERRIT_BRANCH https://test-gerrit/TP/GWApp GWApp
  mv GWApp/.git .
  rm -rf GWApp
else
  git fetch origin $GERRIT_BRANCH
fi

git fetch origin refs/changes/68/64868/4
git checkout -f FETCH_HEAD -- TrunkPackRam/acInfrastructure/MCA
git checkout -f FETCH_HEAD -- TrunkPackRam/Bsps/BspOcteon/Include/bspWanPortDef.h
#git checkout -f origin/$GERRIT_BRANCH -- TrunkPackRam/acInfrastructure/MCA
#git checkout -f origin/$GERRIT_BRANCH -- TrunkPackRam/Bsps/BspOcteon/Include/bspWanPortDef.h
cd $CurrentPath

sudo chmod -R 755 .
sudo chown -R centos:centos .

./build_openrg --board=${BOARD}

cp  $WORKSPACE/$EXTERNAL_VERSION/external_version_data.h $WORKSPACE/OpenRG_external_version_data.h

if (( `du ${BOARD}.OpenRG.img.gz | cut -f1` &lt; 7000 )); then
  echo &quot;${BOARD}.OpenRG.img.gz Image is too small.. exit with error&quot;
  exit -1
fi
</command>
    </hudson.tasks.Shell>
