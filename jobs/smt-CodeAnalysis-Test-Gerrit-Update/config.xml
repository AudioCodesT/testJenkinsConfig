  <builders>
    <hudson.tasks.Shell>
      <command>cat &lt;&lt;-EOF &gt; ~/.netrc
machine test-gerrit login jenkins password $jpass
machine gerrit login jenkins password $jpass
EOF

cd ${WORKSPACE}/..
rm -rf tools
git -c http.sslverify=false clone -n https://gerrit/devops/tools

cd ${WORKSPACE}
</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>## Get cppcheck tools
cd ${WORKSPACE}/..
cd tools

git checkout -f origin/master -- cppcheck/cppreview.sh
git checkout -f origin/master -- cppcheck/suppressions.txt
#git checkout -f origin/master -- cppcheck/exclude.txt

cp cppcheck/* ${WORKSPACE}/

cd ${WORKSPACE}

## Process cppcheck report:
## Running cppcheck with option files from above and generating a flat text file report.
## The report is processed and avote on Gerrit is done according to these results.
. ./cppreview.sh


cd ${WORKSPACE}</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>## Get sputnik/pdm tools
cd ${WORKSPACE}/..
cd tools

git checkout -f origin/master -- sputnik/test-gerrit/smt-pmd.properties
git checkout -f origin/master -- pmd/audc-ruleset.xml

cp sputnik/test-gerrit/smt-pmd.properties ${WORKSPACE}/
cp pmd/audc-ruleset.xml ${WORKSPACE}/

cd ${WORKSPACE}

## Run Sputnik
#sed -i &apos;s/pmd\.enabled\=true/pmd\.enabled\=false/g&apos; /home/centos/jenkinsslave/test-gerrit.properties
#cat /home/centos/jenkinsslave/test-gerrit.properties
#sed -i &apos;s/^pmd\.ruleSets.*$/pmd\.ruleSets\=\/home\/centos\/jenkinsslave\/basic\.xml/g&apos; /home/centos/jenkinsslave/test-gerrit.properties
cat smt-pmd.properties

/opt/sputnik/bin/sputnik --conf ${WORKSPACE}/smt-pmd.properties --changeId $GERRIT_CHANGE_ID --revisionId $GERRIT_PATCHSET_REVISION || { echo &quot;*****   sputnik failed   *****&quot; &amp;&amp; exit 0; }


cd ${WORKSPACE}
</command>
    </hudson.tasks.Shell>
