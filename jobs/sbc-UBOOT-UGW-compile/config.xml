    <hudson.tasks.Shell>
      <command>BL=`git describe --tags | cut -d - -f1,2 | sed s/-/./g`
echo BL ver is: ${BL}
echo COMP_VER=${BL} &gt; generated_version.properties

GERRIT_BRANCH=`git branch --no-color -r --contains HEAD | grep -v -e &apos;-&gt;&apos; | head -1 | awk -F&apos;/&apos; &apos;{print $NF}&apos;`
echo GERRIT_BRANCH=${GERRIT_BRANCH} &gt;&gt; generated_version.properties

# Set SUBLEVEL and EXTRAVERSION in version.mk
# If BL is 0.11, SUBLEVEL=0 and EXTRAVERSION=11
SUBLEVEL=$(echo $BL | cut -d. -f1)
EXTRAVERSION=$(echo $BL | cut -d. -f2)
sed -i.bak -e &apos;s:\(SUBLEVEL = \)[0-9]*:\1&apos;$SUBLEVEL&apos;:; s:\(EXTRAVERSION = \)[0-9]*:\1&apos;$EXTRAVERSION&apos;:&apos; version.mk

cat version.mk

# Check if a previous TAG exist - if not, take all the entire log
PreTAG=$(git describe --tags --abbrev=0 HEAD^) || echo No previous TAG
if [ -z $PreTAG ]; then 
  git log --decorate &gt; ChangeLog.txt
else 
  git log --decorate $PreTAG..$BL &gt; ChangeLog.txt
fi

sudo chown -R centos:centos .
# Execute the build
./buildAll.sh

mv u-boot-serial.asc u-boot-serial-${BL}.asc
mv u-boot-nand.bin u-boot-nand-${BL}.bin
mv u-boot-nand.tar.gz u-boot-nand-${BL}.tar.gz
</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.1.6">
