    <hudson.tasks.Shell>
      <command>BL=`git describe --tags`
echo BL ver is: ${BL}
echo COMP_VER=${BL} &gt; generated_version.properties

echo &quot;#define EXTERNAL_VERSION \&quot;$BL\&quot;&quot; &gt; $WORKSPACE/pkg/version


</command>
