#echo &quot;exit 0 workaround&quot;

# Run Sputnik pmd
cat &lt;&lt;-EOF &gt; ~/.netrc
machine test-gerrit login jenkins password $jpass
machine gerrit login jenkins password $jpass
EOF

cd ${WORKSPACE}/..
rm -rf tools
git -c http.sslverify=false clone -n https://gerrit/devops/tools

cd tools

git checkout -f origin/master -- sputnik/test-gerrit/ems-ovoc-pdm.properties

cp sputnik/test-gerrit/ems-ovoc-pdm.properties ${WORKSPACE}/

cd ${WORKSPACE}

## Run Sputnik
cat ems-ovoc-pdm.properties
/home/centos/sputnik/bin/sputnik --conf ${WORKSPACE}/ems-ovoc-pdm.properties --changeId $GERRIT_CHANGE_ID --revisionId $GERRIT_PATCHSET_REVISION || { echo &quot;*****   sputnik failed   *****&quot; &amp;&amp; exit 0; }


</command>
