  <builders>
    <hudson.tasks.Shell>
      <command>BL=`git describe --tags | cut -d - -f1,2 | sed s/-/./g`
echo BL ver is: ${BL}
echo COMP_VER=${BL} &gt; generated_version.properties
if [ -z "$BRANCH" ]; then
  GERRIT_BRANCH=`git branch --no-color -r --contains HEAD | grep -v -e '-&gt;' | head -1 | awk -F'/' '{print $NF}'`
else
  GERRIT_BRANCH=$BRANCH
fi

echo GERRIT_BRANCH=${GERRIT_BRANCH} &gt;&gt; generated_version.properties


# Check if a previous TAG exist - if not, take all the entire log
PreTAG=$(git describe --tags --abbrev=0 HEAD^) || echo No previous TAG
if [ -z $PreTAG ]; then 
  git log --decorate &gt; ChangeLog.txt
else 
  git log --decorate $PreTAG..$BL &gt; ChangeLog.txt
fi
</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.3.0">
      <info>
        <propertiesFilePath>generated_version.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Shell>
      <command>sudo chmod -R 755 .
sudo chown -R centos:centos .

./build_uboot
zip -mj ${OutputFileNameM4000}_${COMP_VER}.zip ${OutputFileNameM4000}*
ls -l ${OutputFileNameM4000}*

./build_uboot M3100
zip -mj ${OutputFileNameM3100}_${COMP_VER}.zip ${OutputFileNameM3100}*
ls -l ${OutputFileNameM3100}*
</command>
    </hudson.tasks.Shell>
