  <builders>
    <hudson.tasks.Shell>
      <command>#!C:\cygwin64\bin\bash.exe --login -i

id

echo &quot;*****************&quot;
cd ${WORKSPACE}

# Find changed files
chFiles=$(git diff-tree --no-commit-id --name-only --diff-filter=ACMRT -r ${GERRIT_PATCHSET_REVISION} | grep -E &apos;\.cs$|\.c$|\.cpp$|\.h$&apos;)
echo &quot;$chFiles&quot;
# Collect targets to which changed files belong into a list
while read f; do
    target=$(echo $f | gawk -F/ &apos;{print $1}&apos;)
    grep -q $target targets
    if [[ &quot;$?&quot; == &quot;1&quot; ]]; then
        echo -n &quot;$target;&quot; &gt;&gt; targets
    fi
done &lt;&lt;&lt; &quot;$chFiles&quot;
# Delete last semicolon
sed -i &apos;s/;$//&apos; targets

echo &quot;***** Targets to verify:&quot;
cat targets

echo &quot;set targets=$(cat targets)&quot; &gt; setTargets.bat</command>
    </hudson.tasks.Shell>
    <hudson.tasks.BatchFile>
      <command>call setTargets.bat&#xd;
&#xd;
call &quot;C:\Program Files (x86)\Microsoft Visual Studio 14.0\Common7\Tools\vsvars32.bat&quot;&#xd;
&#xd;
&quot;C:\Windows\Microsoft.NET\Framework\v4.0.30319\MSBuild.exe&quot; /p:Configuration=Release VocaServices.sln -target:%targets%</command>
    </hudson.tasks.BatchFile>
