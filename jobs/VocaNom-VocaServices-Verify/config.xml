  <builders>
    <hudson.tasks.Shell>
      <command>#!C:\cygwin64\bin\bash.exe --login -

id

echo "*****************"
cd ${WORKSPACE}

# Find changed files
chFiles=$(git diff-tree --no-commit-id --name-only --diff-filter=ACMRT -r ${GERRIT_PATCHSET_REVISION} | grep -E '\.cs$|\.c$|\.cpp$|\.h$')
echo "Changed files:"
echo "$chFiles"
# Collect targets to which changed files belong into a list
[[ -f targets ]] &amp;&amp; rm -f targets || touch targets
while read f; do
    target=$(echo $f | gawk -F/ '{print $1}')
    [[ -d "$target" ]] &amp;&amp; echo "Found target $target to build" || exit 2
    grep -q $target targets
    if [[ "$?" == "1" ]]; then
        echo -n "$target;" &gt;&gt; targets
    fi
done &lt;&lt;&lt; "$chFiles"

# Delete last semicolon
sed -i 's/;$//' targets

## Output targets to build
echo "***** Targets to build:"
cat targets

echo "set targets=$(cat targets)" &gt; setTargets.bat</command>
    </hudson.tasks.Shell>
    <hudson.tasks.BatchFile>
      <command>call setTargets.bat&#13;
&#13;
call "C:\Program Files (x86)\Microsoft Visual Studio 14.0\Common7\Tools\vsvars32.bat"&#13;
&#13;
"C:\Program Files (x86)\MSBuild\14.0\Bin\MSBuild.exe" /p:Configuration=Release VocaServices.sln -target:%targets%</command>
    </hudson.tasks.BatchFile>
