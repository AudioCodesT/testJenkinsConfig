    <hudson.tasks.Shell>
      <command>artifactory_host=artifactory
User="$artifactory_user:$artifactory_password"

commit_sha1=`git rev-parse HEAD`
count=`curl -k -s -u $User "https://$artifactory_host/api/search/prop?vcs.revision=$commit_sha1&amp;repos=sbc-repo" | grep uri | cut -d'"' -f4 | wc -l`

if (( $count == 7 )); then 
  echo The files there already
  exit 0
fi</command>
    </hudson.tasks.Shell>
    <hudson.tasks.Shell>
      <command>VRXDir="/netapp1/CompilationResults/VRX"

# Should copy the following 5 files:
# 1) 4 VRX.* ( move to TPApp/MPCBin )
# 2) fullimage_A.img, fullimage_B.img
# 3) adsl_vrx_ext_version_data.h ( move to Networking/ApplicationsAndProtocols/CLI/adsl_vrx_ext_version_data.h )
if (( `ls $VRXDir | wc -l` &lt; 7 )); then
  echo "VRX files do not found...."
  exit -1;
fi

mv $VRXDir/* .

if ! [ -e adsl_vrx_ext_version_data.h ]; then mv external_version_data.h adsl_vrx_ext_version_data.h; fi

BL=`git describe --tags` # Add "--first-parent"  after upgrading GIT version in qa-linux-h3
echo BL ver is: ${BL}
echo COMP_VER=${BL} &gt; generated_version.properties

</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.3.0">
      <info>
        <propertiesFilePath>generated_version.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
