    <hudson.tasks.Shell>
      <command>#/bin/sh
#run script make iso branded
#cd ${WORKSPACE}/EMS/VersionRelease
#. branding_iso.sh

#sh brandedscript.sh DVD3_EMS_${TAG} ${COMPANY}


#/bin/sh

version=${TAG}
dvd3_iso=DVD3_EMS_$version.iso
major=${BRANCH}
company=${COMPANY}
cd /home/centos
curl -k -u ems-build:emspass -X GET https://artifactory/ems-release/com/ac/ems/ovoc_server/&quot;$major&quot;/official_iso/&quot;$dvd3_iso&quot;?archiveType=iso &gt;&quot;$dvd3_iso&quot;
curl -k -u ems-build:emspass -X GET https://artifactory/ems-externals/com/ac/ems/branding/&quot;$company&quot;/about_logo.png&gt;about_logo.png

curl -k -u ems-build:emspass -X GET https://artifactory/ems-externals/com/ac/ems/branding/&quot;$company&quot;/logo_login.png&gt;logo_login.png

curl -k -u ems-build:emspass -X GET https://artifactory/ems-externals/com/ac/ems/branding/&quot;$company&quot;/logo.png&gt;logo.png

chmod 755 about_logo.png
chmod 755 logo_login.png
chmod 755 logo.png




rm -rf /home/centos/original_iso

mkdir /home/centos/original_iso

sudo mount  -o rw,loop -t iso9660 &quot;$dvd3_iso&quot; /home/centos/original_iso
cp -avr /home/centos/original_iso/ /home/centos/copy_original_iso/

cp -f /home/centos/logo.png /home/centos/copy_original_iso/EmsServerInstall/ac_ems_deploy/server_&quot;$version&quot;/web-ui-ovoc/assets/images/logo.png
cp -f /home/centos/about_logo.png /home/centos/copy_original_iso/EmsServerInstall/ac_ems_deploy/server_&quot;$version&quot;/web-ui-ovoc/assets/images/about_logo.png
cp -f /home/centos/logo_login.png /home/centos/copy_original_iso/EmsServerInstall/ac_ems_deploy/server_&quot;$version&quot;/web-ui-ovoc/assets/images/logo_login.png
cp -f /home/centos/about_logo.png /home/centos/copy_original_iso/EmsServerInstall/ac_ems_deploy/server_&quot;$version&quot;/web-ui-ovoc/images/about_logo.png
echo ${COMPANY} &gt;/home/centos/copy_original_iso/EmsServerInstall/ac_ems_deploy/server_&quot;$version&quot;/web-ui-ovoc/branding.txt

if [ $COMPANY == Nuera ]; then
mkdir Documentation
curl -k -u ems-build:emspass -X GET https://artifactory/ems-externals/com/ac/ems/branding/&quot;$company&quot;/Docs/&apos;299-090-550 One Voice Operations Center Release Notes Ver. 7.6.pdf&apos;?archiveType=pdf&gt;Documentation/&apos;299-090-550 One Voice Operations Center Release Notes Ver. 7.6.pdf&apos;
curl -k -u ems-build:emspass -X GET https://artifactory/ems-externals/com/ac/ems/branding/&quot;$company&quot;/Docs/&apos;299-091-046 One Voice Operations Center Users Manual Ver. 7.6.pdf&apos;?archiveType=pdf&gt;Documentation/&apos;299-091-046 One Voice Operations Center Users Manual Ver. 7.6.pdf&apos;
curl -k -u ems-build:emspass -X GET https://artifactory/ems-externals/com/ac/ems/branding/&quot;$company&quot;/Docs/&apos;299-094-168 One Voice Operations Center IOM Manual Ver. 7.6.pdf&apos;?archiveType=pdf&gt;Documentation/&apos;299-094-168 One Voice Operations Center IOM Manual Ver. 7.6.pdf&apos;
chmod -R 755 Documentation
rm -rf /home/centos/copy_original_iso/Documentation
cp -rf Documentation/ /home/centos/copy_original_iso/
fi

rm -rf DVD3_EMS_&quot;$version&quot;.iso
mkisofs -V DVD3_EMS -o DVD3_EMS_&quot;$version&quot;.iso -R -J -joliet-long copy_original_iso
</command>
