                    <buildSteps>
                      <hudson.tasks.Shell>
                        <command>
# They are sbc-sanity-build that running now?
echo &quot;Check if they are sbc-sanity-build that running now&quot;
for job in $(curl -ks &quot;https://test-jenkins/computer/api/json?tree=computer\[executors\[currentExecutable\[url\]\],\
oneOffExecutors\[currentExecutable\[url\]\]\]&quot; | \
sed -e &apos;s/&quot;url&quot;:&quot;/\n/g&apos; | sed &apos;s/&quot;}}.*//g&apos; | tail -n +2 | grep sbc-sanity-build); do
