  <builders>
    <hudson.tasks.Shell>
      <command>BL=`git describe --tags`
echo BL ver is: ${BL}
echo OPEN_RG_VER=${BL} &gt; generated_openRG_version.properties

echo &quot;#define EXTERNAL_VERSION \&quot;$BL\&quot;&quot; &gt; $WORKSPACE/pkg/version</command>
    </hudson.tasks.Shell>
    <EnvInjectBuilder plugin="envinject@2.1.6">
      <info>
        <propertiesFilePath>generated_openRG_version.properties</propertiesFilePath>
      </info>
    </EnvInjectBuilder>
    <hudson.tasks.Shell>
      <command>
if [ ! -d &quot;$WORKSPACE/CompilationResults&quot; ]; then
  rm -rf $WORKSPACE/CompilationResults
fi

#echo &quot;machine test-gerrit login jenkins password $jpass&quot; &gt;~/.netrc
cat ~/.netrc
GERRIT_BRANCH=`git branch --no-color -r --contains HEAD | grep -v -e &apos;-&gt;&apos; | head -1 | awk -F&apos;origin/&apos; &apos;{print $NF}&apos;`
cd ../..
if [ ! -d .git ]; then
  git -c http.sslverify=false clone --depth 10 -n -b $GERRIT_BRANCH https://test-gerrit/TP/GWApp GWApp
  mv GWApp/.git .
  rm -rf GWApp
else
  git fetch origin $GERRIT_BRANCH
fi

git checkout -f origin/$GERRIT_BRANCH -- TrunkPackRam OpenSource 
git checkout -f origin/$GERRIT_BRANCH -- Shared || true    
cd $WORKSPACE

sudo chmod -R 755 .
sudo chown -R centos:centos .

./build_grub2.sh --server=localhost
cp  $WORKSPACE/pkg/version $WORKSPACE/hostedbl_external_version_data.h

if (( `du ssbc-bootloader.pkg | cut -f1` &lt; 1000 )); then
  echo &quot;ssbc-bootloader.pkg Image too small.. exit with error&quot;
  exit -1
fi
</command>
    </hudson.tasks.Shell>
